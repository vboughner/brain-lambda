{
  "name": "brain-lambda",
  "version": "1.5.0",
  "description": "My Brain lambda for interacting with Bixby Capsule",
  "main": "index.js",
  "scripts": {
    "deploy":"./deploy.sh",
    "layer": "./layer.sh",
    "postinstall": "cd src && yarn install",
    "clean": "rm -rf index.zip nodejs.zip nodejs",
    "test": "echo cd src && yarn test",
    "watch": "echo cd src && yarn run watch",
    "memorize": "node src/cmdline.js memorize",
    "recall": "node src/cmdline.js recall",
    "list": "node src/cmdline.js list",
    "delete-all": "node src/cmdline.js delete-all",
    "delete-one": "node src/cmdline.js delete-one",
    "get-report": "node src/cmdline.js get-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vboughner/brain-lambda.git"
  },
